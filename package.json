{
  "name": "clusternator",
  "version": "0.0.11",
  "description": "Program to deploy docker containers on cloud service platforms with minimal user input, and/or configuration",
  "main": "clusternator.js",
  "preferGlobal": true,
  "keywords": [
    "docker", "aws", "amazon", "cluster", "cloud", "heroku", "vm",
    "virtual machine", "container"
  ],
  "bugs": {
    "url": "https://github.com/rangle/the-clusternator/issues"
  },
  "scripts": {
    "start": "./bin/clusternator.sh server:start",
    "test": "gulp test-unit",
    "build": "gulp transpile",
    "prepublish": "npm run build",
    "docker-build": "npm run build && docker build -t rafkhan/clusternator-test .",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/the-clusternator"
  },
  "author": "rangle.io",
  "license": "BSD-2-Clause",
  "dependencies": {
    "aws-sdk": "^2.2.12",
    "body-parser": "^1.14.1",
    "express": "^4.13.3",
    "q": "^1.4.1",
    "ramda": "^0.17.1",
    "request-promise": "^0.4.3",
    "superagent": "^1.4.0",
    "winston": "^1.1.1",
    "yargs": "^3.29.0"
  },
  "bin": {
    "clusternator": "./bin/clusternator.sh"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "eslint": "^1.8.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.3.0",
    "gulp-eslint": "^1.0.0",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "mocha": "^2.3.3",
    "rewire": "^2.3.4",
    "supertest": "^1.1.0"
  }
}
